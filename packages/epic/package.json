  "bugs": {
    "url": "https://github.com/hitosu/duckness/issues"
  },
  "dependencies": {
    "@duckness/duck": "*"
  },
  "peerDependencies": {
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.7"
