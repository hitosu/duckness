{
  "name": "duckness",
  "version": "0.3.4",
  "description": "Duckness - Modular Redux Ducks monorepo",
  "main": "index.js",
  "repository": "git@github.com:hitosu/duckness.git",
  "author": "Vasily Kazantsev <vasily@kazantsev.xyz>",
  "license": "MIT",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "test": "yarn lint && jest",
    "test:watch": "yarn lint && jest --watch",
    "lint": "lerna exec yarn lint && eslint 'stories/**/*.{js,jsx}'",
    "build": "lerna bootstrap && lerna exec --parallel -- yarn build",
    "rebuild": "lerna bootstrap && lerna exec --parallel -- yarn rebuild",
    "build:watch": "lerna bootstrap && lerna exec --parallel -- yarn build:watch",
    "lerna:publish": "lerna publish --force-publish=*",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn build && yarn test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addons": "^6.1.15",
    "@storybook/cli": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "redux-saga": "^1.1.3"
  }
}
