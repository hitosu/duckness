{
  "name": "duckness",
  "version": "0.3.4",
  "description": "Duckness - Modular Redux Ducks monorepo",
  "main": "index.js",
  "repository": "git@github.com:hitosu/duckness.git",
  "author": "Vasily Kazantsev <vasily@kazantsev.xyz>",
  "license": "MIT",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "test": "yarn lint && jest",
    "test:watch": "yarn lint && jest --watch",
    "lint": "lerna exec yarn lint && eslint 'stories/**/*.{js,jsx}'",
    "build": "lerna bootstrap && lerna exec --parallel -- yarn build",
    "rebuild": "lerna bootstrap && lerna exec --parallel -- yarn rebuild",
    "build:watch": "lerna bootstrap && lerna exec --parallel -- yarn build:watch",
    "lerna:publish": "lerna publish --force-publish=*",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addons": "^6.3.2",
    "@storybook/cli": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "lerna": "^4.0.0",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "react": ">= 16",
    "react-redux": ">= 7",
    "redux": ">= 4",
    "redux-observable": ">= 1",
    "redux-saga": ">= 1"
  },
  "resolutions": {
    "trim": ">= 1",
    "trim-newlines": ">= 4.0.1",
    "glob-parent": ">= 5.1.2",
    "browserslist": ">= 4.16.6"
  }
}
